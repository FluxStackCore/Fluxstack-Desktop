{
  "name": "@fluxstack/desktop",
  "version": "1.0.0",
  "description": "FluxStack Desktop - Create native desktop applications from web apps using system browsers and Bun runtime.",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run --watch example.ts",
    "demo": "bun fluxworld/index.ts",
    "test": "bun simple-test.ts",
    "example": "bun example.ts",
    "build": "bun build src/index.ts --outdir dist --target bun --sourcemap && bun run tsc --emitDeclarationOnly",
    "typecheck": "bun run tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcosBrendonDePaula/FluxStack.git"
  },
  "author": "FluxStack Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarcosBrendonDePaula/FluxStack/issues"
  },
  "homepage": "https://github.com/MarcosBrendonDePaula/FluxStack#readme",
  "type": "module",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "^1.0.0",
    "typescript": "^5.0.0"
  },
  "trustedDependencies": [
    "bun-types"
  ]
}